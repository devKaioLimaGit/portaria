generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Portaria {
  id            Int       @id @default(autoincrement())
  portaria      String?
  filename      String?
  nome          String?
  cpf           String?
  cargo         String?
  simbolo       String?
  secretaria    String?
  tipoDocumento String?
  data          DateTime?
  dataNomeacao  DateTime?

  @@map("portarias")
}

model Users {
  id       Int    @id @default(autoincrement())
  nome     String
  cpf      String
  password String
  role     String

  @@map("users")
}
